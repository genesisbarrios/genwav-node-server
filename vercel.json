{
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node",
        "config": {
          "env": {
            "PORT": 3000 
          }
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "app.js" }
    ]
  }
  